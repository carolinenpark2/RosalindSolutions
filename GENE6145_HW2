#DNA
dna = 'AGCTTTTCATTCTGACTGCAACGGGCAATATGTCTCTGTGTGGATTAAAAAAAGAGTGTCTGATAGCAGC'

def count_dna(s):
    count_a = s.count('A')
    count_c = s.count('C')
    count_g = s.count('G')
    count_t = s.count('T')
    return count_a, count_c, count_g, count_t

count_dna(dna)

#RNA
dna = 'GATGGAACTTGACTACGTAAATT'

def transcribe_dna(t):
    return t.replace('T', 'U')

transcribe_dna(dna)
